  "keywords": [
    "svelte",
    "svelvet",
    "typescript"
  ],
  "authors": "Anu, Aaron, Alexander, Andrew, Ali, Damian, Justin, Von, Walter, Jeffrey, David, Samee, Christopher, Johnny, Abhi, Ian, Mauricio, Ryan",
