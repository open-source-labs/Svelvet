{
	"name": "svelvet",
	"version": "0.0.7",
	"description": "A lightweight Svelte component library for building interactive node-based flow diagrams",
	"authors": "Anu, Aaron, Alexander, Andrew",
	"license": "MIT",
	"bugs": "https://github.com/oslabs-beta/Svelvet/issues",
	"homepage": "https://github.com/oslabs-beta/Svelvet",
	"repository": {
		"type": "git",
		"url": "https://github.com/oslabs-beta/Svelvet"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "vitest run tests/unit && playwright test tests/e2e",
		"test:unit": "vitest tests/unit",
		"test:e2e": "playwright test tests/e2e",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.21.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/svelte-repl": "^0.4.1",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/svelte": "^3.1.1",
		"@types/d3-zoom": "^3.0.1",
		"@types/jest": "^27.5.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.7",
		"d3-selection": "^3.0.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^19.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.5.9",
		"svelvet": "^0.0.7",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2",
		"vitest": "^0.12.9"
	},
	"type": "module",
	"main": "./index.js",
	"dependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"d3-zoom": "^3.0.0"
	}
}
